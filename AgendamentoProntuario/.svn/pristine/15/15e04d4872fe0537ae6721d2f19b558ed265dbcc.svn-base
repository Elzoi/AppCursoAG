<div class="wrapper">
    <form 
    #formLogin="ngForm" 
    class="form-signin"
    (ngSubmit)="autenticarUsuario()"
    >       
        <h2 class="form-signin-heading">Acesso do Servidor</h2>
        <input type="text" class="form-control" name="cpfUsuario" placeholder="CPF" required="" autofocus="" />
        <input type="text" class="form-control" name="matriculaUsuario" placeholder="Matricula" required=""/>      
        <button class="btn btn-lg btn-primary btn-block" type="submit">Logar</button>   
    </form>
  </div>